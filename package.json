{
  "name": "systemjs-tools",
  "version": "1.2.11",
  "description": "(dev)tools for working with SystemJS",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel lib -d dist",
    "dev": "babel -w lib -d dist",
    "prepublish": "npm run build"
  },
  "registry": "npm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexisvincent/systemjs-tools.git"
  },
  "bin": {
    "systemjs": "./dist/cli.js"
  },
  "keywords": [
    "JSPM",
    "dev",
    "tools",
    "devtools",
    "systemjs"
  ],
  "author": "Alexis Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexisvincent/systemjs-tools/issues"
  },
  "homepage": "https://github.com/alexisvincent/systemjs-tools#readme",
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "jspm": "beta"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "bluebird": "latest",
    "chokidar": "^1.6.1",
    "graceful-fs": "latest",
    "jspm-npm": "^0.30.1",
    "socket.io": "^1.5.0",
    "spdy": "^3.4.4",
    "spdy-keys": "0.0.0",
    "systemjs-config-builder": "~0.0.2",
    "systemjs-hmr": "^0.1.3"
  },
  "peerDependencies": {
    "assert": "jspm-nodelibs-assert@^0.2.0",
    "bcrypt-pbkdf": "^1.0.0",
    "buffer": "jspm-nodelibs-buffer@^0.2.0",
    "child_process": "jspm-nodelibs-child_process@^0.2.0",
    "cluster": "jspm-nodelibs-cluster@^0.2.0",
    "console": "jspm-nodelibs-console@^0.2.1",
    "constants": "jspm-nodelibs-constants@^0.2.0",
    "crypto": "jspm-nodelibs-crypto@^0.2.0",
    "dgram": "jspm-nodelibs-dgram@^0.2.0",
    "dns": "jspm-nodelibs-dns@^0.2.0",
    "domain": "jspm-nodelibs-domain@^0.2.0",
    "ecc-jsbn": "~0.1.1",
    "events": "jspm-nodelibs-events@^0.2.0",
    "fs": "jspm-nodelibs-fs@^0.2.0",
    "fsevents": "^1.0.0",
    "http": "jspm-nodelibs-http@^0.2.0",
    "https": "jspm-nodelibs-https@^0.2.1",
    "jodid25519": "^1.0.0",
    "jsbn": "0.1",
    "module": "jspm-nodelibs-module@^0.2.0",
    "net": "jspm-nodelibs-net@^0.2.0",
    "os": "jspm-nodelibs-os@^0.2.0",
    "path": "jspm-nodelibs-path@^0.2.1",
    "process": "jspm-nodelibs-process@^0.2.0",
    "punycode": "jspm-nodelibs-punycode@^0.2.0",
    "querystring": "jspm-nodelibs-querystring@^0.2.0",
    "readline": "jspm-nodelibs-readline@^0.2.0",
    "repl": "jspm-nodelibs-repl@^0.2.0",
    "stream": "jspm-nodelibs-stream@^0.2.0",
    "string_decoder": "jspm-nodelibs-string_decoder@^0.2.0",
    "timers": "jspm-nodelibs-timers@^0.2.0",
    "tls": "jspm-nodelibs-tls@^0.2.0",
    "tty": "jspm-nodelibs-tty@^0.2.0",
    "tweetnacl": "0.14",
    "url": "jspm-nodelibs-url@^0.2.0",
    "util": "jspm-nodelibs-util@^0.2.1",
    "vm": "jspm-nodelibs-vm@^0.2.0",
    "zlib": "jspm-nodelibs-zlib@^0.2.0"
  },
  "jspm": {
    "main": "dist/client.js",
    "overrides": {
      "npm:bluebird@3.4.6": {
        "meta": {
          "js/browser/bluebird.js": {
            "format": "global"
          },
          "js/browser/bluebird.min.js": {
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:debug@2.3.2": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:engine.io@1.7.2": {
        "dependencies": {
          "base64id": "0.1.0",
          "debug": "2.2.0",
          "ws": "1.0.1",
          "engine.io-parser": "1.2.4",
          "accepts": "1.3.1"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.2": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:readable-stream@1.1.14": {
        "map": {
          "stream": "stream-browserify/index"
        },
        "systemjs": {
          "main": "readable.js"
        },
        "dependencies": {
          "core-util-is": "~1.0.0",
          "inherits": "~2.0.1",
          "isarray": "0.0.1",
          "string_decoder": "~0.10.0",
          "stream-browserify": "^1.0.0"
        }
      },
      "npm:socket.io-client@1.5.1": {
        "main": "lib/index.js"
      },
      "npm:socket.io@1.5.1": {
        "map": {
          "socket.io-client/package": "socket.io-client/package.json"
        }
      },
      "npm:ws@1.0.1": {
        "dependencies": {
          "options": ">=0.0.5",
          "ultron": "1.0.x",
          "utf-8-validate": "1.2.x",
          "bufferutil": "1.2.x"
        }
      },
      "npm:ws@1.1.1": {
        "dependencies": {
          "options": ">=0.0.5",
          "ultron": "1.0.x",
          "utf-8-validate": "1.2.x",
          "bufferutil": "1.2.x"
        }
      }
    }
  }
}
